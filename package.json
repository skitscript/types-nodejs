{
  "name": "@skitscript/types-nodejs",
  "description": "A Skitscript document parser targeting NodeJS.",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.14.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/skitscript/types-nodejs"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all2": "^5.0.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildLint": "eslint \"**/*.ts\"",
    "buildLintFix": "eslint --fix \"**/*.ts\"",
    "buildTypeScript": "tsc",
    "test": "npm-run-all buildClean buildLintFix buildTypeScript"
  },
  "files": [
    "**/index.js",
    "**/index.d.ts"
  ],
  "types": "index.d.ts",
  "sideEffects": false,
  "type": "module"
}
